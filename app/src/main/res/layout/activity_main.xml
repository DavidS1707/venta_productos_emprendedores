<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PRESENTACION.MainActivity">

    <Button
        android:id="@+id/btnRegistrarEmprendimiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emprendimiento"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintBottom_toTopOf="@+id/btnCatalogos"
        android:layout_marginTop="32dp" />

    <Button
        android:id="@+id/btnCatalogos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CatÃ¡logos"
        app:layout_constraintTop_toBottomOf="@+id/btnRegistrarEmprendimiento"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintBottom_toTopOf="@+id/btnProductos"
        android:layout_marginTop="32dp" />

    <Button
        android:id="@+id/btnProductos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Productos"
        app:layout_constraintTop_toBottomOf="@+id/btnCatalogos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintBottom_toTopOf="@+id/btnNotasVenta"
        android:layout_marginTop="32dp" />

    <Button
        android:id="@+id/btnNotasVenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notas de venta"
        app:layout_constraintTop_toBottomOf="@+id/btnProductos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="32dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
